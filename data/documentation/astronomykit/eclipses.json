":"swift","type":"codeListing","code":["let eclipse = try Eclipse.searchLocalSolar(after: .now, from: observer)","","print(\"Partial begins: \\(eclipse.partialBegin.time)\")","print(\"Peak: \\(eclipse.peak.time)\")","print(\"Partial ends: \\(eclipse.partialEnd.time)\")","","\/\/ For total\/annular eclipses","if let totalBegin = eclipse.totalBegin,","   let totalEnd = eclipse.totalEnd {","    print(\"Totality: \\(totalBegin.time) to \\(totalEnd.time)\")","}"]},{"text":"Eclipse Visibility","type":"heading","level":3,"anchor":"Eclipse-Visibility"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Each event includes altitude information:"}]},{"syntax":"swift","type":"codeListing","code":["let event = eclipse.peak","print(\"Time: \\(event.time)\")","print(\"Sun altitude: \\(event.altitude)°\")","","if event.isVisible {","    print(\"Sun is above the horizon\")","}","","if eclipse.isVisible {","    print(\"At least part of the eclipse is visible\")","}"]},{"text":"All Local Eclipses in a Range","type":"heading","level":3,"anchor":"All-Local-Eclipses-in-a-Range"},{"syntax":"swift","type":"codeListing","code":["let observer = Observer(latitude: 35.0, longitude: -120.0)","let eclipses = try Eclipse.localSolarEclipses(","    from: .now,","    to: .now.addingDays(365 * 10),","    from: observer",")","","for eclipse in eclipses {","    if eclipse.obscuration > 0.5 {","        print(\"Notable eclipse: \\(Int(eclipse.obscuration * 100))% on \\(eclipse.peak.time)\")","    }","}"]},{"text":"Eclipse Search Tips","type":"heading","level":2,"anchor":"Eclipse-Search-Tips"},{"type":"orderedList","items":[{"content":[{"inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Search far ahead"}]},{"type":"text","text":": Eclipses are rare—search years in advance"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"inlineContent":[{"text":"Check visibility","type":"text"}],"type":"strong"},{"text":": Local eclipses may occur when the Sun is